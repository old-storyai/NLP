{
    "month": {
        "jan(uary)?": 0,
        "feb(ruary)?": 1,
        "mar(ch)?": 2,
        "apr(il)?": 3,
        "may": 4,
        "jun(e)?": 5,
        "jul(y)?": 6,
        "aug(ust)?": 7,
        "sep(tember)?": 8,
        "oct(ober)?": 9,
        "nov(ember)?": 10,
        "dec(ember)?": 11,

        "\\d+[/\\-\\.](\\d+)[/\\-\\.]\\d+": "$1"
    },
    "year": {
        "(\\d{4})": "$1",
        "\\d+[/\\-\\.]\\d+[/\\-\\.](\\d+)": "$1"
    },
    "day": {
        "(\\d+)st": "$1",
        "(\\d+)nd": "$1",
        "(\\d+)rd": "$1",
        "(\\d+)th": "$1",
        "(\\d+)[/\\-\\.]\\d+[/\\-\\.]\\d+": "$1"
    },
    "weekDay": {
        "mon(day)?": 0,
        "tue(sday)?": 1,
        "wed(nesday)?": 2,
        "thu(rsday)?": 3,
        "fri(day)?": 4,
        "sat(urday)?": 5,
        "sun(day)?": 6
    },
    "hour": {
        "(\\d)+ hours?": "$1",
        "(\\d+):\\d+(:\\d+)?": "$1"
    },
    "minute": {
        "(\\d)+ minutes?": "$1",
        "\\d+:(\\d+)(:\\d+)?": "$1"
    },
    "second": {
        "(\\d)+ seconds?": "$1",
        "\\d+:\\d+(:\\d+)": "$1"
    }
}
