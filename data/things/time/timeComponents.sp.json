{
    "(^| )(\\d+)( |$)": "~$2",

    "(\\d{4})": "Y=$1",
    "\\d+[/\\-\\.]\\d+[/\\-\\.](\\d+)": "Y=$1",

    "years?": "~Y",
    "this year":     "Y={o0{year, 0}}",



    "jan(uary)?":   "M=0",
    "feb(ruary)?":  "M=1",
    "mar(ch)?":     "M=2",
    "apr(il)?":     "M=3",
    "may":          "M=4",
    "jun(e)?":      "M=5",
    "jul(y)?":      "M=6",
    "aug(ust)?":    "M=7",
    "sep(tember)?": "M=8",
    "oct(ober)?":   "M=9",
    "nov(ember)?":  "M=10",
    "dec(ember)?":  "M=11",
    "\\d+[/\\-\\.](\\d+)([/\\-\\.]\\d+)?": "M={={$1-1}}",

    "this month":     "M={o0{month, 0}}",
    "months?": "~M",


    "(\\d+)st": "D=$1",
    "(\\d+)nd": "D=$1",
    "(\\d+)rd": "D=$1",
    "(\\d+)th": "D=$1",
    "(\\d+)[/\\-\\.]\\d+([/\\-\\.]\\d+)?": "D=$1",

    "today": "D={o0{day,0}}",
    "tomorrow": "D={o0{day,0}} ++> D~1",
    "yesterday": "D={o0{day,0}} --> D~1",
    "days?": "~D",

    "this week": "W={o0{week, 0, start}}",
    "every weeks?": "D~7",
    "(\\d+) weeks?": "D~{={$1*7}}",


    "midnight": "h=0",
    "noon": "h=12",
    "(\\d+):\\d+(:\\d+)?": "h=$1",
    "(\\d+)h\\d+": "h=$1",
    "(\\d+)(:\\d+)?AM": "h=$1",
    "(\\d+)(:\\d+)?PM": "h={={$1+12}}",

    "hours?": "~h",


    "\\d+:(\\d+)(:\\d+)?": "m=$1",
    "\\d+h(\\d+)": "m=$1",
    "minutes?": "~m",


    "\\d+:\\d+:(\\d+)": "s=$1",
    "seconds?": "~s",



    "after": "<++",
    "before": "<--",

    "ago": "<-- D={o0{d,0}}",
    "in": "D={o0{d,0}} ++>",

    "every": "% ~1",
    "each": "% ~1",

    "for": "$",
    "until": "<--",

    "between": "<++",
    "and": "<--",
    "from": "<++",
    "to": "<--",

    "next": "?={o0{d,0}} ++> ~1",
    "last": "?={o0{d,0}} --> ~-1",

    "night": "h=19",
    "tonight": "h=19 D={o0{day,0}}",
    "this (morning|afternoon)": "D={o0{day,0}}",
    "morning": "h=9",
    "afternoon": "h=15",

    "mondays?":    "@2000-01-03#D%D~7",
    "tuesdays?":   "@2000-01-04#D%D~7",
    "wednesdays?": "@2000-01-05#D%D~7",
    "thursdays?":  "@2000-01-06#D%D~7",
    "fridays?":    "@2000-01-07#D%D~7",
    "saturdays?":  "@2000-01-08#D%D~7",
    "sundays?":    "@2000-01-09#D%D~7",
    "christmas": "@2000-12-25#D%Y~1",
    "new year":  "@2000-01-01T00:00:00#D%Y~1",
    "Baptiste 's birthday":  "@1997-12-18T00:00:00#D%Y~1"
}
